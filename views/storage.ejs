<!DOCTYPE html>
<html lang="en">
<%- include("partials/headTag", {title: "Odin storage", style: "storage"}) %>
<body>
	<script src="uploads.js" defer></script>
	<%- include("partials/header") %>
	<%- include("partials/navBar") %>
	<ul>
		<% items.forEach(element => { %>
		<li>
			<a href="#"><%= `${element.name} is ${element.hasOwnProperty("folderId")? "file": "folder"}` %></a>
		</li>
		<% }); %>
	</ul>
	<%- include("partials/uploadsForm", { route: childFolder }) %>
</body>
</html>